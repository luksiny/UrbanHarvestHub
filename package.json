{
  "name": "urban-harvest-hub",
  "version": "1.0.0",
  "description": "Full-Stack PWA for Urban Harvest Hub",
  "main": "backend/index.js",
  "scripts": {
    "start": "node backend/index.js",
    "dev": "concurrently -n server,client \"npm run server\" \"npm run client\"",
    "server": "nodemon backend/index.js",
    "client": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd frontend && npm install",
    "seed": "node backend/seed.js"
  },
  "keywords": [
    "pwa",
    "fullstack",
    "react",
    "express",
    "mysql",
    "sequelize"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
